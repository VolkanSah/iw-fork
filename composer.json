{
	"name": "iw-reload/iw",
	"description": "IceWars Reload",
	"keywords": ["icewars", "browsergame"],
	"type": "project",
	"license": "GNU AFFERO GENERAL PUBLIC LICENSE",
	"support": {
		"issues": "https://github.com/iw-reload/iw/issues?state=open",
		"forum": "http://www.icewars-forum.de/",
		"source": "https://github.com/iw-reload/iw"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-authclient": "*"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*",
		"codeception/codeception": " 2.0.*",
		"codeception/specify": " *",
		"codeception/verify": " *",
		"yiisoft/yii2-faker": " *"
	},
	"suggest": {
		"codeception/codeception": "Codeception, 2.0.* is currently works well with Yii.",
		"codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
		"codeception/verify": "BDD Assertions for PHPUnit and Codeception",
		"yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"backend/runtime",
			"backend/web/assets",

			"frontend/runtime",
			"frontend/web/assets"
		]
	}
}
