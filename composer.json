{
	"name": "iw-reload/iw",
	"description": "IceWars Reload",
	"keywords": ["icewars", "browsergame"],
	"type": "project",
	"license": "GNU AFFERO GENERAL PUBLIC LICENSE",
	"support": {
		"issues": "https://github.com/iw-reload/iw/issues?state=open",
		"forum": "http://www.icewars-forum.de/",
		"source": "https://github.com/iw-reload/iw"
	},
	"minimum-stability": "dev",
	"require": {
    "doctrine/orm": "2.5.*",
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
    "yiisoft/yii2-authclient": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-jui": "*",
		"yiisoft/yii2-swiftmailer": "*"
	},
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
